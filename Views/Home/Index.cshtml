@{
    ViewBag.Title = "Home Page";
}


<!-- Start slider -->
<section id="aa-slider">
    <div class="aa-slider-area">
        <div id="sequence" class="seq">
            <div class="seq-screen">
                <ul class="seq-canvas">
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/2.jpg" alt="Wristwatch slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/3.jpg" alt="Women Jeans slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/4.jpg" alt="Shoes slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/5.jpg" alt="Male Female slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/6.gif" alt="Male Female slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/1.jpg" alt="Male Female slide img" />
                        </div>
                    </li>
                    <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/7.jpg" alt="Male Female slide img" />
                        </div>
                    </li>   <!-- single slide item -->
                    <li>
                        <div class="seq-model">
                            <img data-seq src="~/Image/Slide/8.jpg" alt="Male Female slide img" />
                        </div>
                    </li>
                </ul>
            </div>
            <!-- slider navigation btn -->
            <fieldset class="seq-nav" aria-controls="sequence" aria-label="Slider buttons">
                <a type="button" class="seq-prev" aria-label="Previous"><span class="fa fa-angle-left"></span></a>
                <a type="button" class="seq-next" aria-label="Next"><span class="fa fa-angle-right"></span></a>
            </fieldset>
        </div>
    </div>
</section>
<!-- / slider -->
<!-- Start Promo section -->
<section id="aa-promo">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-promo-area">
                    <div class="row">
                        <!-- promo right -->
                        <div class="col-md-7 no-padding">
                            <div class="aa-promo-right">
                                @Html.Action("Group", "Home")
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Promo section -->
<!-- Products section -->
<section id="aa-product">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="aa-product-area">
                        <div class="aa-product-inner">
                            <!-- start prduct navigation -->
                            <ul class="nav nav-tabs aa-products-tab">
                                <li class="active"><a href="#men" data-toggle="tab">Đồ điện tử</a></li>
                                <li><a href="#women" data-toggle="tab">Thiết bị nghe nhìn</a></li>
                                <li><a href="#sports" data-toggle="tab">Đồ gia dụng</a></li>
                                <li><a href="#electronics" data-toggle="tab">Xe cộ</a></li>
                                <li><a href="#electronics" data-toggle="tab">Sách-báo</a></li>
                            </ul>
                            <!-- Tab panes -->
                            <div class="tab-content">
                                <!-- Start men product category -->
                                <div class="tab-pane fade in active" id="men">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @{Html.RenderAction("ShowProductDienTu", "Home");}
                                        <!-- start single product item -->
                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("index","Product")">Xem thêm <span class="fa fa-long-arrow-right"></span></a>
                                </div>
                                <!-- / men product category -->
                                <!-- start women product category -->
                                <div class="tab-pane fade" id="women">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @{Html.RenderAction("ShowProductNgehNhin", "Home");}
                                        <!-- start single product item -->
                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("index","Product")">Xem thêm <span class="fa fa-long-arrow-right"></span></a>
                                </div>
                                <!-- / women product category -->
                                <!-- start sports product category -->
                                <div class="tab-pane fade" id="sports">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @{Html.RenderAction("ShowProductGiaDung", "Home");}
                                        <!-- start single product item -->
                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("index","Product")">Xem thêm  <span class="fa fa-long-arrow-right"></span></a>
                                </div>
                                <!-- / sports product category -->
                                <!-- start electronic product category -->
                                <div class="tab-pane fade" id="electronics">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @{Html.RenderAction("ShowProductXeCo", "Home");}
                                        <!-- start single product item -->
                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("index","Product")">Xem thêm  <span class="fa fa-long-arrow-right"></span></a>
                                </div>
                                <!-- / electronic product category -->
                                <!-- start electronic product category -->
                                <div class="tab-pane fade" id="sach">
                                    <ul class="aa-product-catg">
                                        <!-- start single product item -->
                                        @{Html.RenderAction("ShowProductSach", "Home");}
                                        <!-- start single product item -->
                                    </ul>
                                    <a class="aa-browse-btn" href="@Url.Action("index","Product")">Xem thêm  <span class="fa fa-long-arrow-right"></span></a>
                                </div>
                                <!-- / electronic product category -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Products section -->
