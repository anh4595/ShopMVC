<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Sida - Mua của người chán bán cho người cần | Website chuyên kinh doanh đồ cũ</title>

    <!-- Font awesome -->
    <link href="~/Assets/UI/css/font-awesome.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/Assets/UI/css/bootstrap.css" rel="stylesheet">
    <!-- SmartMenus jQuery Bootstrap Addon CSS -->
    <link href="~/Assets/UI/css/jquery.smartmenus.bootstrap.css" rel="stylesheet">
    <!-- Product view slider -->
    <link rel="stylesheet" type="text/css" href="/Assets/UI/css/jquery.simpleLens.css">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="~/Assets/UI/css/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="~/Assets/UI/css/nouislider.css">
    <!-- Theme color -->
    <link id="switcher" href="~/Assets/UI/css/theme-color/default-theme.css" rel="stylesheet">
    <!-- <link id="switcher" href="css/theme-color/bridge-theme.css" rel="stylesheet"> -->
    <!-- Top Slider CSS -->
    <link href="~/Assets/UI/css/sequence-theme.modern-slide-in.css" rel="stylesheet" media="all">

    <!-- Main style sheet -->
    <link href="~/Assets/UI/css/style.css" rel="stylesheet">
    <link href="~/Assets/Awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Assets/UI/css/hovertop.css" rel="stylesheet" />
    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>
<body>
    <!-- wpf loader Two -->
    <div id="wpf-loader-two">
        <div class="wpf-loader-two-inner">
            <span>Loading</span>
        </div>
    </div>
    <!-- / wpf loader Two -->
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-chevron-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start header section -->
    <header id="aa-header">
       @{Html.RenderAction("TopMenu", "Home");}
        <!-- start header bottom  -->
        <div class="aa-header-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="aa-header-bottom-area">
                            <!-- logo  -->
                            <div class="aa-logo">
                                <!-- Text based logo -->
                                <a href="@Url.Action("index","Home")">
                                    <span class="fa fa-shopping-cart"></span>
                                    <p>Sida<strong>Shop</strong> <span>Mua của người chán - bán cho người cần</span></p>
                                </a>
                                <!-- img based logo -->
                                <!-- <a href="index.html"><img src="img/logo.jpg" alt="logo img"></a> -->
                            </div>
                            <!-- / logo  -->
                             @{Html.RenderAction("HeaderCar", "Home");}
                            <!-- search box -->
                            <div class="aa-search-box">
                                <form action="">
                                    <input type="text" name="" id="" placeholder="Nhập từ khóa cần tìm kiếm ...">
                                    <button type="submit"><span class="fa fa-search"></span></button>
                                </form>
                            </div>
                            <!-- / search box -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- / header bottom  -->
    </header>
    <!-- / header section -->
    <!-- menu -->
    <section id="menu">
        <div class="container">
            <div class="menu-area">
                <!-- Navbar -->
                <div class="navbar navbar-default" role="navigation">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="navbar-collapse collapse">
                        <!-- Left nav -->
                        <ul class="nav navbar-nav">
                            <li><a href="@Url.Action("index","home")">Trang Chủ</a></li>
                            <li><a href="@Url.Action("index","News")">Tin Tức</a></li>
                            @{Html.RenderAction("MMenuCProduct", "Home"); }
                            <li><a href="@Url.Action("Contact","Home")">Liên Hệ</a></li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </div>
        </div>
    </section>
    <!-- / menu -->
    @RenderBody()
    <!-- footer -->
    <footer id="aa-footer">
        <!-- footer bottom -->
        <div class="aa-footer-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="aa-footer-top-area">
                            <div class="row">

                                <div class="col-md-3 col-sm-6">
                                    <div class="aa-footer-widget">
                                        <h3>DANH MỤC</h3>
                                        <ul class="aa-footer-nav">
                                            <li><a href="@Url.Action("index","Home")">Trang Chủ</a></li>
                                            <li><a href="@Url.Action("index","Product")">Sản Phẩm</a></li>
                                            <li><a href="@Url.Action("Contact","Home")">Liên hệ</a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6">
                                    <div class="aa-footer-widget">
                                        <div class="aa-footer-widget">
                                            <h3>LIÊN HỆ</h3>
                                            <address>
                                                <p> 250A Điện Biên Phủ, P.25, Q.Bình Thạnh, TP.Hồ Chí Minh</p>
                                                <p><span class="fa fa-phone"></span>+84 166-822-0682</p>
                                                <p><span class="fa fa-envelope"></span>sidashop@gmail.com</p>
                                            </address>
                                            <div class="aa-footer-social">
                                                <a href="#"><span class="fa fa-facebook"></span></a>
                                                <a href="#"><span class="fa fa-twitter"></span></a>
                                                <a href="#"><span class="fa fa-google-plus"></span></a>
                                                <a href="#"><span class="fa fa-youtube"></span></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6" style="margin-left:-300px">
                                    <section id="aa-subscribe">
                                        <div class="container">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="aa-subscribe-area">
                                                        <h3 style="color:#ffffff; font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">ĐĂNG KÝ NHẬN TIN </h3>
                                                        <p style="color:#ffffff;font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif">Đăng ký nhận tin từ Sida và nhận nhiều voucher hấp dẫn</p>
                                                        <form action="" class="aa-subscribe-form">
                                                            <input type="email" name="" id="" placeholder="Địa chỉ Email của bạn">
                                                            <input type="submit" value="Đăng ký">
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- footer-bottom -->
        <div class="aa-footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="aa-footer-bottom-area">
                            <p>&copy Copyright <a href="http://www.markups.io/">SidaShop</a></p>
                            <div class="aa-footer-payment">
                                <span class="fa fa-cc-mastercard" title="Master Card"></span>
                                <span class="fa fa-cc-visa" title="Visa Card"></span>
                                <span class="fa fa-paypal" title="Paypal"></span>
                                <span class="fa fa-cc-discover" title="Discover"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- / footer -->

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Assets/UI/js/bootstrap.js"></script>
    <!-- SmartMenus jQuery plugin -->
    <script type="text/javascript" src="~/Assets/UI/js/jquery.smartmenus.js"></script>
    <!-- SmartMenus jQuery Bootstrap Addon -->
    <script type="text/javascript" src="~/Assets/UI/js/jquery.smartmenus.bootstrap.js"></script>
    <!-- To Slider JS -->
    <script src="~/Assets/UI/js/sequence.js"></script>
    <script src="~/Assets/UI/js/sequence-theme.modern-slide-in.js"></script>
    <!-- Product view slider -->
    <script type="text/javascript" src="/Assets/UI/js/jquery.simpleGallery.js"></script>
    <script type="text/javascript" src="/Assets/UI/js/jquery.simpleLens.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="~/Assets/UI/js/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="~/Assets/UI/js/nouislider.js"></script>
    <!-- Custom js -->
    <script src="~/Assets/UI/js/custom.js"></script>

</body>
</html>