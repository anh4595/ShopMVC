
@{
    Layout = null;
}
@model IEnumerable<Model.EntityFramework.ORDERDETAIL>

@foreach (var item in Model)
{
    <li>
        <a href="@Url.Action("ProductDetail", "Product", new { id = item.PRODUCT_ID })" class="aa-cartbox-img"><img alt="img" src="~/Image/HinhSanPham/images/Product/@item.PRODUCT.IMAGE_LINK"></a>
        <div class="aa-cartbox-info">
            <h4><a href="@Url.Action("ProductDetail", "Product", new { id = item.PRODUCT_ID })">@item.PRODUCT.NAME</a></h4>
            <p>1 x $@item.PRODUCT.DISCOUNT_PRICE</p>
        </div>
    </li>
}
